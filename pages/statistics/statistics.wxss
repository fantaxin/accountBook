.page {
    background-color: white;
}

.total {
    position: relative;
    left: 5rpx;
    top: 10rpx;
    width: 750rpx;
    height: 100rpx;
    display: inline-block;
    padding: 0rpx;
    background-color: white;
    z-index: 1;
}

.line {
    color: #00CE84;
    font-weight: bolder;
    font-size: larger;
}

.title {
    margin-left: 10rpx;
    color: black;
    font-weight: bold;
    font-family: "Fira Mono";
    font-size: medium;

}

.money {
    position: relative;
    left: 50rpx;
    top: 10rpx;
    width: fit-content;
    height: auto;
    display: block;
    box-sizing: border-box;
    padding: 0rpx;
    padding-left: 0rpx;
    margin-bottom: 20rpx;
    color: black;
    font-family: "Fira Mono";
    font-size: large;
}

.columnData {
    position: relative;
    left: 0rpx;
    top: 40rpx;
    width: 750rpx;
    height: auto;
    display: inline-block;
    padding: 0rpx;
    background-color: white;
    z-index: 1;
}

.ringData {
    position: relative;
    left: 0rpx;
    top: 25rpx;
    width: 750rpx;
    height: auto;
    display: inline-block;
    padding: 0rpx;
    margin-bottom: 200rpx;
    background-color: white;
    z-index: 1;
}

.canvas {
    width: 100%;
    height: 410rpx;
    display: inline-block;
    top: 20rpx;
}

.canvas2 {
    position: relative;
    top: 20rpx;
}


.text {
    position: absolute;
    left: 35rpx;
    top: 0rpx;
    width: fit-content;
    height: auto;
    display: block;
    box-sizing: border-box;
    padding: 0rpx;
    padding-left: 0rpx;
    color: black;
    font-family: "Fira Mono";
    font-size: x-large;
}